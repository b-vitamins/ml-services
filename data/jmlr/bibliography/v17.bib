 abstract = {We define a general framework for a large class of combinatorial multi-armed bandit (CMAB) problems, where subsets of base arms with unknown distributions form super arms. In each round, a super ar...},
 author = {Wei Chen and Yajun Wang and Yang Yuan and Qinshi Wang},
 journal = {Journal of Machine Learning Research},

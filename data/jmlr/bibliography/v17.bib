 abstract = {Information criteria have been popularly used in model selection and proved to possess nice theoretical properties. For classification, Claeskens et al. (2008) proposed support vector machine infor...},
 author = {Xiang Zhang and Yichao Wu and Lan Wang and Runze Li},
 journal = {Journal of Machine Learning Research},

 publisher = {PMLR},
 series = {Proceedings of Machine Learning Research},
 title = {Testing Symmetric Markov Chains Without Hitting.},
 url = {https://proceedings.mlr.press/v99/cherapanamjeri19a.html},
 volume = {99},

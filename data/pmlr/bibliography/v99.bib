 publisher = {PMLR},
 series = {Proceedings of Machine Learning Research},
 title = {Learning from Weakly Dependent Data under Dobrushinâs Condition},
 url = {https://proceedings.mlr.press/v99/dagan19a.html},
 volume = {99},
 year = {2019}
}


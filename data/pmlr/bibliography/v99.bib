 publisher = {PMLR},
 series = {Proceedings of Machine Learning Research},
 title = {Sampling and Optimization on Convex Sets in Riemannian Manifolds of Non-Negative Curvature},
 url = {https://proceedings.mlr.press/v99/goyal19a.html},
 volume = {99},
 year = {2019}
}


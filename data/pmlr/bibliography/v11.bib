 publisher = {PMLR},
 series = {Proceedings of Machine Learning Research},
 title = {$\mu$TOSS - Multiple hypothesis testing in an open software system},
 url = {https://proceedings.mlr.press/v11/blanchard10a.html},
 volume = {11},
 year = {2010}

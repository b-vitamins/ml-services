 address = {Cumberland Lodge, Windsor, UK},
 author = {Blanchard, Gilles and Dickhaus, Thorsten and Hack, Niklas and Konietschke, Frank and Rohmeyer, Kornelius and Rosenblatt, Jonathan and Scheer, Marsel and Werft, Wiebke},

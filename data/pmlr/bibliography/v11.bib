@inproceedings{pmlr-v11-ali10a,
 abstract = {In this article we present an application of text-analysis technologies to support social science research, in particular the analysis of patterns in news content. We describe a system that gathers and annotates large volumes of textual data in order to extract patterns and trends. We have examined 3.5 million news articles and show that their topic is related to the gender bias and readability of their content. This study is intended to illustrate how pattern analysis technology can be deployed to automate tasks commonly performed by humans in the social sciences, in order to enable large scale studies that would otherwise be impossible.},
 address = {Cumberland Lodge, Windsor, UK},
 author = {Ali, Omar and Flaounas, Ilias and Bie, Tijl De and Mosdell, Nick and Lewis, Justin and Cristianini, Nello},
 booktitle = {Proceedings of the First Workshop on Applications of Pattern Analysis},
 editor = {Diethe, Tom and Cristianini, Nello and Shawe-Taylor, John},
 month = {01--03 Sep},
 openalex = {W2112611142},
 pages = {36--43},
 pdf = {http://proceedings.mlr.press/v11/ali10a/ali10a.pdf},
 publisher = {PMLR},
 series = {Proceedings of Machine Learning Research},
 title = {Automating News Content Analysis: An Application to Gender Bias and Readability},
 url = {https://proceedings.mlr.press/v11/ali10a.html},
 volume = {11},
 year = {2010}
}

@inproceedings{pmlr-v11-ariu10a,
 abstract = {Zero-days attacks are one of the most dangerous threats against computer networks. These, by denition, are attacks never seen before. Thus, defense tools based on a database of rules (usually referred as \signatures) that describe known attacks cannot do anything against them. Recently, defense tools based on machine learning algorithms have gained an increasing popularity as they oer the possibility to ght o also zero-days attacks. In this paper we propose HMMPayl, an anomaly based Intrusion Detection System for the protection of a web server and of the applications the server hosts. HMMPayl analyzes the network trac toward the web server and it is based on Hidden Markov Models. With this paper we provide for several contributions. First, the algorithm implemented by HMMPayl allows to carefully model the payload increasing the classication accuracy with respect to previously proposed solutions. Second, we show that an approach based on multiple classiers leads to an increased classication accuracy with respect to the case where a single classier is used. Third, exploiting the redundancy within the information extracted from the payload we propose a solution to reduce the computational cost of the algorithm.},
 address = {Cumberland Lodge, Windsor, UK},
 author = {Ariu, Davide and Giacinto, Giorgio},
 booktitle = {Proceedings of the First Workshop on Applications of Pattern Analysis},
 editor = {Diethe, Tom and Cristianini, Nello and Shawe-Taylor, John},
 month = {01--03 Sep},
 openalex = {W2185583596},
 pages = {81--87},
 pdf = {http://proceedings.mlr.press/v11/ariu10a/ariu10a.pdf},
 publisher = {PMLR},
 series = {Proceedings of Machine Learning Research},
 title = {HMMPayl: an application of HMM to the analysis of the HTTP Payload},
 url = {https://proceedings.mlr.press/v11/ariu10a.html},
 volume = {11},
 year = {2010}
}

@inproceedings{pmlr-v11-auer10a,
 abstract = {This paper describes Pinview, a content-based image retrieval system that exploits implicit relevance feedback during a search session. Pinview contains several novel methods that infer the intent of the user. From relevance feedback, such as eye movements or clicks, and visual features of images Pinview learns a similarity metric between images which depends on the current interests of the user. It then retrieves images with a specialized reinforcement learning algorithm that balances the tradeo between exploring new images and exploiting the already inferred interests of the user. In practise, we have integrated Pinview to the content-based image retrieval system PicSOM, in order to apply it to realworld image databases. Preliminary experiments show that eye movements provide a rich input modality from which it is possible to learn the interests of the user.},
 address = {Cumberland Lodge, Windsor, UK},
 author = {Auer, Peter and Hussain, Zakria and Kaski, Samuel and Klami, Arto and Kujala, Jussi and Laaksonen, Jorma and Leung, Alex P. and Pasupa, Kitsuchart and Shawe-Taylor, John},
 booktitle = {Proceedings of the First Workshop on Applications of Pattern Analysis},
 editor = {Diethe, Tom and Cristianini, Nello and Shawe-Taylor, John},
 month = {01--03 Sep},
 openalex = {W2097329790},
 pages = {51--57},
 pdf = {http://proceedings.mlr.press/v11/auer10a/auer10a.pdf},
 publisher = {PMLR},
 series = {Proceedings of Machine Learning Research},
 title = {Pinview: Implicit Feedback in Content-Based Image Retrieval},
 url = {https://proceedings.mlr.press/v11/auer10a.html},
 volume = {11},
 year = {2010}
}

@inproceedings{pmlr-v11-balcazar10a,
 abstract = {We focus on association rule mining. It is well-known that naive miners end up often providing far too large amounts of mined associations to result actually useful in practice. Many proposals exist for selecting appropriate association rules, trying to measure their interest in various ways; most of these approaches are statistical in nature, or share their main traits with statistical notions. Alternatively, some existing notions of redundancy among association rules allow for a logical-style characterization and lead to irredundant bases (axiomatizations) of absolutely minimum size. Here we follow up on a study of closure-based redundancy, which, in practice, leads to smaller bases than simpler alternative forms of redundancy, with the proviso that, in principle, they need to be complemented with an implicational basis. One can push the intuition of redundancy further and gain a perspective of the interest of association rules in terms of their \novelty with respect to other rules. An irredundant rule is so because its condence is higher than what the rest of the rules would suggest; then, one can ask: how much higher? Among several variants, a recently proposed parameter, the condence boost, succeeds in measuring a notion of novelty along these lines so that it ts better the needs of practical applications. However, that notion is based on plain redundancy, of relatively limited practical usefulness. Here we extend the condence boost to closure-based redundancy, paying a small theoretical price to obtain several advantages in practical applications. We describe a rule-mining system implementing this contribution.},
 address = {Cumberland Lodge, Windsor, UK},
 author = {BalcÃ¡zar, JosÃ© L.},
 booktitle = {Proceedings of the First Workshop on Applications of Pattern Analysis},
 editor = {Diethe, Tom and Cristianini, Nello and Shawe-Taylor, John},
 month = {01--03 Sep},
 openalex = {W9619735},
 pages = {74--80},
 pdf = {http://proceedings.mlr.press/v11/balcazar10a/balcazar10a.pdf},
 publisher = {PMLR},
 series = {Proceedings of Machine Learning Research},
 title = {Closure-Based Confidence Boost in Association Rules},
 url = {https://proceedings.mlr.press/v11/balcazar10a.html},
 volume = {11},
 year = {2010}
}


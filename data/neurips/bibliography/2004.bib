 publisher = {MIT Press},
 title = {Object Classification from a Single Example Utilizing Class Relevance Metrics},
 url = {https://proceedings.neurips.cc/paper_files/paper/2004/hash/ef1e491a766ce3127556063d49bc2f98-Abstract.html},
 volume = {17},

 publisher = {MIT Press},
 title = {The power of feature clustering: An application to object detection},
 url = {https://proceedings.neurips.cc/paper_files/paper/2004/hash/06fe1c234519f6812fc4c1baae25d6af-Abstract.html},
 volume = {17},

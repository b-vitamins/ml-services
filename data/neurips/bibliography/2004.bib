 pdf = {https://proceedings.neurips.cc/paper_files/paper/2004/file/743c41a921516b04afde48bb48e28ce6-Paper.pdf},
 publisher = {MIT Press},
 title = {Reducing Spike Train Variability: A Computational Theory Of Spike-Timing Dependent Plasticity.},
 url = {https://proceedings.neurips.cc/paper_files/paper/2004/hash/743c41a921516b04afde48bb48e28ce6-Abstract.html},
 volume = {17},

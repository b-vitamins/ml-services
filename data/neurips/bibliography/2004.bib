 openalex = {W2139302369},
 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2004/file/02f657d55eaf1c4840ce8d66fcdaf90c-Paper.pdf},
 publisher = {MIT Press},
 title = {Learning first-order Markov models for control},
 url = {https://proceedings.neurips.cc/paper_files/paper/2004/hash/02f657d55eaf1c4840ce8d66fcdaf90c-Abstract.html},

 title = {Neural Network Computation by In Vitro Transcriptional Circuits},
 url = {https://proceedings.neurips.cc/paper_files/paper/2004/hash/65f2a94c8c2d56d5b43a1a3d9d811102-Abstract.html},
 volume = {17},

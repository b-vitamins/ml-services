 pdf = {https://proceedings.neurips.cc/paper_files/paper/2004/file/f1748d6b0fd9d439f71450117eba2725-Paper.pdf},
 publisher = {MIT Press},
 title = {Modeling Conversational Dynamics as a Mixed-Memory Markov Process},
 url = {https://proceedings.neurips.cc/paper_files/paper/2004/hash/f1748d6b0fd9d439f71450117eba2725-Abstract.html},
 volume = {17},
 year = {2004}
}


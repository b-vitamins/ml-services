 publisher = {MIT Press},
 title = {Common-Frame Model for Object Recognition},
 url = {https://proceedings.neurips.cc/paper_files/paper/2004/hash/d37124c4c79f357cb02c655671a432fa-Abstract.html},
 volume = {17},

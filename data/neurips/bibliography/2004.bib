 publisher = {MIT Press},
 title = {Using Random Forests in the Structured Language Model},
 url = {https://proceedings.neurips.cc/paper_files/paper/2004/hash/7edccc661418aeb5761dbcdc06ad490c-Abstract.html},
 volume = {17},

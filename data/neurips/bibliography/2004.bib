 publisher = {MIT Press},
 title = {Co-Validation: Using Model Disagreement on Unlabeled Data to Validate Classification Algorithms},
 url = {https://proceedings.neurips.cc/paper_files/paper/2004/hash/92f54963fc39a9d87c2253186808ea61-Abstract.html},
 volume = {17},

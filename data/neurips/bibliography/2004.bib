 publisher = {MIT Press},
 title = {Methods for Estimating the Computational Power and Generalization Capability of Neural Microcircuits},
 url = {https://proceedings.neurips.cc/paper_files/paper/2004/hash/9ff7c9eb9d37f434db778f59178012da-Abstract.html},
 volume = {17},

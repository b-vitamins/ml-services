 publisher = {MIT Press},
 title = {Chemosensory Processing in a Spiking Model of the Olfactory Bulb: Chemotopic Convergence and Center Surround Inhibition},
 url = {https://proceedings.neurips.cc/paper_files/paper/2004/hash/7e83722522e8aeb7512b7075311316b7-Abstract.html},
 volume = {17},

 publisher = {MIT Press},
 title = {A Phase Space Approach to Minimax Entropy Learning and the Minutemax Approximations},
 url = {https://proceedings.neurips.cc/paper_files/paper/1998/hash/bc573864331a9e42e4511de6f678aa83-Abstract.html},
 volume = {11},

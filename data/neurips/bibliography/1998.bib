 publisher = {MIT Press},
 title = {Applications of Multi-Resolution Neural Networks to Mammography},
 url = {https://proceedings.neurips.cc/paper_files/paper/1998/hash/b5488aeff42889188d03c9895255cecc-Abstract.html},
 volume = {11},

 publisher = {MIT Press},
 title = {Linear Hinge Loss and Average Margin},
 url = {https://proceedings.neurips.cc/paper_files/paper/1998/hash/a14ac55a4f27472c5d894ec1c3c743d2-Abstract.html},
 volume = {11},

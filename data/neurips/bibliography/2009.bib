 publisher = {Curran Associates, Inc.},
 title = {Noisy Generalized Binary Search},
 url = {https://proceedings.neurips.cc/paper_files/paper/2009/hash/556f391937dfd4398cbac35e050a2177-Abstract.html},
 volume = {22},

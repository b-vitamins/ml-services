 pdf = {https://proceedings.neurips.cc/paper_files/paper/2009/file/0188e8b8b014829e2fa0f430f0a95961-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {White Functionals for Anomaly Detection in Dynamical Systems},
 url = {https://proceedings.neurips.cc/paper_files/paper/2009/hash/0188e8b8b014829e2fa0f430f0a95961-Abstract.html},

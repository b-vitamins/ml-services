 openalex = {W2158535911},
 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2009/file/437d7d1d97917cd627a34a6a0fb41136-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {Nonparametric Latent Feature Models for Link Prediction},
 url = {https://proceedings.neurips.cc/paper_files/paper/2009/hash/437d7d1d97917cd627a34a6a0fb41136-Abstract.html},
 volume = {22},
 year = {2009}

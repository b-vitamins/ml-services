 pdf = {https://proceedings.neurips.cc/paper_files/paper/2009/file/f197002b9a0853eca5e046d9ca4663d5-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {Structural inference affects depth perception in the context of potential occlusion},
 url = {https://proceedings.neurips.cc/paper_files/paper/2009/hash/f197002b9a0853eca5e046d9ca4663d5-Abstract.html},
 volume = {22},

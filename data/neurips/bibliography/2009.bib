 publisher = {Curran Associates, Inc.},
 title = {STDP enables spiking neurons to detect hidden causes of their inputs},
 url = {https://proceedings.neurips.cc/paper_files/paper/2009/hash/a5cdd4aa0048b187f7182f1b9ce7a6a7-Abstract.html},
 volume = {22},

 publisher = {Curran Associates, Inc.},
 title = {Modeling Social Annotation Data with Content Relevance using a Topic Model},
 url = {https://proceedings.neurips.cc/paper_files/paper/2009/hash/39059724f73a9969845dfe4146c5660e-Abstract.html},
 volume = {22},

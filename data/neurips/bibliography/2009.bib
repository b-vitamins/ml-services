 pdf = {https://proceedings.neurips.cc/paper_files/paper/2009/file/e2a2dcc36a08a345332c751b2f2e476c-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {Learning a Small Mixture of Trees},
 url = {https://proceedings.neurips.cc/paper_files/paper/2009/hash/e2a2dcc36a08a345332c751b2f2e476c-Abstract.html},
 volume = {22},
 year = {2009}

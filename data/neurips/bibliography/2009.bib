 openalex = {W2168345951},
 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2009/file/043c3d7e489c69b48737cc0c92d0f3a2-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {Slow, Decorrelated Features for Pretraining Complex Cell-like Networks},
 url = {https://proceedings.neurips.cc/paper_files/paper/2009/hash/043c3d7e489c69b48737cc0c92d0f3a2-Abstract.html},
 volume = {22},

 publisher = {Curran Associates, Inc.},
 title = {Rank-Approximate Nearest Neighbor Search: Retaining Meaning and Speed in High Dimensions},
 url = {https://proceedings.neurips.cc/paper_files/paper/2009/hash/ddb30680a691d157187ee1cf9e896d03-Abstract.html},
 volume = {22},

 pdf = {https://proceedings.neurips.cc/paper_files/paper/2009/file/5ea1649a31336092c05438df996a3e59-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {Inter-domain Gaussian Processes for Sparse Inference using Inducing Features},
 url = {https://proceedings.neurips.cc/paper_files/paper/2009/hash/5ea1649a31336092c05438df996a3e59-Abstract.html},

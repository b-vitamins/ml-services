 openalex = {W1759689472},
 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2009/file/68d30a9594728bc39aa24be94b319d21-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {Maximin affinity learning of image segmentation},
 url = {https://proceedings.neurips.cc/paper_files/paper/2009/hash/68d30a9594728bc39aa24be94b319d21-Abstract.html},
 volume = {22},

 openalex = {W2105728138},
 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2009/file/428fca9bc1921c25c5121f9da7815cde-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {Measuring Invariances in Deep Networks},
 url = {https://proceedings.neurips.cc/paper_files/paper/2009/hash/428fca9bc1921c25c5121f9da7815cde-Abstract.html},
 volume = {22},

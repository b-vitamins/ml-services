 publisher = {Curran Associates, Inc.},
 supplemental = {https://proceedings.neurips.cc/paper_files/paper/2009/file/685ac8cadc1be5ac98da9556bc1c8d9e-Supplemental.zip},
 title = {Kernel Choice and Classifiability for RKHS Embeddings of Probability Distributions},
 url = {https://proceedings.neurips.cc/paper_files/paper/2009/hash/685ac8cadc1be5ac98da9556bc1c8d9e-Abstract.html},

 openalex = {W2114352343},
 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2009/file/efe937780e95574250dabe07151bdc23-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {A Bayesian Analysis of Dynamics in Free Recall},
 url = {https://proceedings.neurips.cc/paper_files/paper/2009/hash/efe937780e95574250dabe07151bdc23-Abstract.html},
 volume = {22},
 year = {2009}

 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2009/file/6e7b33fdea3adc80ebd648fffb665bb8-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {3D Object Recognition with Deep Belief Nets},
 url = {https://proceedings.neurips.cc/paper_files/paper/2009/hash/6e7b33fdea3adc80ebd648fffb665bb8-Abstract.html},
 volume = {22},

 publisher = {Curran Associates, Inc.},
 supplemental = {https://proceedings.neurips.cc/paper_files/paper/2009/file/9ad6aaed513b73148b7d49f70afcfb32-Supplemental.zip},
 title = {Submodularity Cuts and Applications},
 url = {https://proceedings.neurips.cc/paper_files/paper/2009/hash/9ad6aaed513b73148b7d49f70afcfb32-Abstract.html},

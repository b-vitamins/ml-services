 publisher = {Curran Associates, Inc.},
 title = {Robust Value Function Approximation Using Bilinear Programming},
 url = {https://proceedings.neurips.cc/paper_files/paper/2009/hash/ec5decca5ed3d6b8079e2e7e7bacc9f2-Abstract.html},
 volume = {22},

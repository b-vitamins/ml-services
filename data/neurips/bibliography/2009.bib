 publisher = {Curran Associates, Inc.},
 title = {Accelerating Bayesian Structural Inference for Non-Decomposable Gaussian Graphical Models},
 url = {https://proceedings.neurips.cc/paper_files/paper/2009/hash/a1519de5b5d44b31a01de013b9b51a80-Abstract.html},
 volume = {22},

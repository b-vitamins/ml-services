 publisher = {Curran Associates, Inc.},
 title = {Ranking Measures and Loss Functions in Learning to Rank},
 url = {https://proceedings.neurips.cc/paper_files/paper/2009/hash/2f55707d4193dc27118a0f19a1985716-Abstract.html},
 volume = {22},

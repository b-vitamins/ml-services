 pdf = {https://proceedings.neurips.cc/paper_files/paper/2009/file/ccb0989662211f61edae2e26d58ea92f-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {A Biologically Plausible Model for Rapid Natural Scene Identification},
 url = {https://proceedings.neurips.cc/paper_files/paper/2009/hash/ccb0989662211f61edae2e26d58ea92f-Abstract.html},

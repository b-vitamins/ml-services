 openalex = {W2097582237},
 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2009/file/6cfe0e6127fa25df2a0ef2ae1067d915-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 supplemental = {https://proceedings.neurips.cc/paper_files/paper/2009/file/6cfe0e6127fa25df2a0ef2ae1067d915-Supplemental.zip},
 title = {Riffled Independence for Ranked Data},
 url = {https://proceedings.neurips.cc/paper_files/paper/2009/hash/6cfe0e6127fa25df2a0ef2ae1067d915-Abstract.html},

 publisher = {Curran Associates, Inc.},
 title = {Toward Provably Correct Feature Selection in Arbitrary Domains},
 url = {https://proceedings.neurips.cc/paper_files/paper/2009/hash/6da37dd3139aa4d9aa55b8d237ec5d4a-Abstract.html},
 volume = {22},

 publisher = {Curran Associates, Inc.},
 title = {A joint maximum-entropy model for binary neural population patterns and continuous signals},
 url = {https://proceedings.neurips.cc/paper_files/paper/2009/hash/be83ab3ecd0db773eb2dc1b0a17836a1-Abstract.html},

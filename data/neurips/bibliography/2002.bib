 publisher = {MIT Press},
 title = {Learning Attractor Landscapes for Learning Motor Primitives},
 url = {https://proceedings.neurips.cc/paper_files/paper/2002/hash/23c97e9cb93576e45d2feaf00d0e8502-Abstract.html},
 volume = {15},
 year = {2002}
}

@inproceedings{NIPS2002_243facb2,

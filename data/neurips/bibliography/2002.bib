@inproceedings{NIPS2002_38651c44,
 abstract = {Cortical synaptic plasticity depends on the relative timing of pre- and postsynaptic spikes and also on the temporal pattern of presynaptic spikes and of postsynaptic spikes. We study the hypothesis that cortical synaptic plasticity does not associate individual spikes, but rather whole episodes, and depends only on when these start and how long they last, but as little as possible on the timing of individual spikes. Here we present the mathematical background for such a study. Standard methods from hidden Markov models are used to define what firing episodes are. Estimating the probability of being in such an episode requires not only the knowledge of past spikes, but also of future spikes. We show how to construct a causal learning rule, which depends only on past spikes, but associates pre- and postsynaptic as if it also knew future spikes. We also show that this learning rule agrees with some features of synaptic plasticity in superficial layers of rat visual cortex (Froemke and Dan, Nature 416:433, 2002).},
 author = {Eisele, Michael and Miller, Kenneth},
 bib = {https://proceedings.neurips.cc/paper_files/paper/2002/file/38651c4450f87348fcbe1f992746a954-Bibtex.bib},
 booktitle = {Advances in Neural Information Processing Systems},
 editor = {S. Becker and S. Thrun and K. Obermayer},
 metadata = {https://proceedings.neurips.cc/paper_files/paper/2002/file/38651c4450f87348fcbe1f992746a954-Metadata.json},
 openalex = {W2131979958},
 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2002/file/38651c4450f87348fcbe1f992746a954-Paper.pdf},
 publisher = {MIT Press},
 title = {Hidden Markov Model of Cortical Synaptic Plasticity: Derivation of the Learning Rule},
 url = {https://proceedings.neurips.cc/paper_files/paper/2002/hash/38651c4450f87348fcbe1f992746a954-Abstract.html},
 volume = {15},
 year = {2002}
}

@inproceedings{NIPS2002_3a1dd983,
 abstract = {We present an automatic alignment procedure which maps the disparate internal representations learned by several local dimensionality reduction experts into a single, coherent global coordinate system for the original data space. Our algorithm can be applied to any set of experts, each of which produces a low-dimensional local representation of a high-dimensional input. Unlike recent efforts to coordinate such models by modifying their objective functions [1, 2], our algorithm is invoked after training and applies an efficient eigensolver to post-process the trained models. The post-processing has no local optima and the size of the system it must solve scales with the number of local models rather than the number of original data points, making it more efficient than model-free algorithms such as Isomap [3] or LLE [4].},

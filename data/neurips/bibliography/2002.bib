 pdf = {https://proceedings.neurips.cc/paper_files/paper/2002/file/d5e2fbef30a4eb668a203060ec8e5eef-Paper.pdf},
 publisher = {MIT Press},
 title = {Inferring a Semantic Representation of Text via Cross-Language Correlation Analysis},
 url = {https://proceedings.neurips.cc/paper_files/paper/2002/hash/d5e2fbef30a4eb668a203060ec8e5eef-Abstract.html},
 volume = {15},

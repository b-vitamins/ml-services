 openalex = {W2126599826},
 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2002/file/f3d9de86462c28781cbe5c47ef22c3e5-Paper.pdf},
 publisher = {MIT Press},
 title = {Maximum Likelihood and the Information Bottleneck},
 url = {https://proceedings.neurips.cc/paper_files/paper/2002/hash/f3d9de86462c28781cbe5c47ef22c3e5-Abstract.html},
 volume = {15},
 year = {2002}
}


 pdf = {https://proceedings.neurips.cc/paper_files/paper/2002/file/9978b7063e297d84bb2ac8e46c1c845f-Paper.pdf},
 publisher = {MIT Press},
 title = {VIBES: A Variational Inference Engine for Bayesian Networks},
 url = {https://proceedings.neurips.cc/paper_files/paper/2002/hash/9978b7063e297d84bb2ac8e46c1c845f-Abstract.html},
 volume = {15},
 year = {2002}
}

@inproceedings{NIPS2002_9f44e956,
 abstract = {We study a new, model-free form of approximate iteration which uses Sarsa updates with linear state-action value function approximation for evaluation, and a policy improvement to generate a new based on the learned state-action values. We prove that if the improvement operator produces e-soft policies and is Lipschitz continuous in the action values, with a constant that is not too large, then the approximate iteration algorithm converges to a unique solution from any initial policy. To our knowledge, this is the first convergence result for any form of approximate iteration under similar computational-resource assumptions.},

 publisher = {MIT Press},
 title = {A Prototype for Automatic Recognition of Spontaneous Facial Actions},
 url = {https://proceedings.neurips.cc/paper_files/paper/2002/hash/fe40fb944ee700392ed51bfe84dd4e3d-Abstract.html},
 volume = {15},

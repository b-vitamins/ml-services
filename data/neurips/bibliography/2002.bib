 pdf = {https://proceedings.neurips.cc/paper_files/paper/2002/file/046ddf96c233a273fd390c3d0b1a9aa4-Paper.pdf},
 publisher = {MIT Press},
 title = {Classifying Patterns of Visual Motion - a Neuromorphic Approach},
 url = {https://proceedings.neurips.cc/paper_files/paper/2002/hash/046ddf96c233a273fd390c3d0b1a9aa4-Abstract.html},
 volume = {15},

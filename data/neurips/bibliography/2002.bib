 title = {Real Time Voice Processing with Audiovisual Feedback: Toward Autonomous Agents with Perfect Pitch},
 url = {https://proceedings.neurips.cc/paper_files/paper/2002/hash/43351f7bf9a215be70c2c2caa7555002-Abstract.html},

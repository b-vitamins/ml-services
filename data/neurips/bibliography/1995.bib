 title = {Clustering data through an analogy to the Potts model},
 url = {https://proceedings.neurips.cc/paper_files/paper/1995/hash/6a2feef8ed6a9fe76d6b3f30f02150b4-Abstract.html},
 volume = {8},
 year = {1995}

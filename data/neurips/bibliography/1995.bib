 publisher = {MIT Press},
 title = {A Framework for Non-rigid Matching and Correspondence},
 url = {https://proceedings.neurips.cc/paper_files/paper/1995/hash/c60d060b946d6dd6145dcbad5c4ccf6f-Abstract.html},
 volume = {8},
 year = {1995}

 publisher = {MIT Press},
 title = {Hierarchical Recurrent Neural Networks for Long-Term Dependencies},
 url = {https://proceedings.neurips.cc/paper_files/paper/1995/hash/c667d53acd899a97a85de0c201ba99be-Abstract.html},
 volume = {8},
 year = {1995}
}


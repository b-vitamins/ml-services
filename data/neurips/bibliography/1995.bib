 title = {SEEMORE: a view-based approach to 3-D object recognition using multiple visual cues},
 url = {https://proceedings.neurips.cc/paper_files/paper/1995/hash/a0e2a2c563d57df27213ede1ac4ac780-Abstract.html},
 volume = {8},

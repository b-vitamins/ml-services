 pdf = {https://proceedings.neurips.cc/paper_files/paper/1987/file/c20ad4d76fe97759aa27a0c99bff6710-Paper.pdf},
 publisher = {American Institute of Physics},
 title = {Using Neural Networks to Improve Cochlear Implant Speech Perception},
 url = {https://proceedings.neurips.cc/paper_files/paper/1987/hash/c20ad4d76fe97759aa27a0c99bff6710-Abstract.html},
 volume = {0},

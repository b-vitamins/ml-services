 publisher = {American Institute of Physics},
 title = {PARTITIONING OF SENSORY DATA BY A CORTICAL NETWORK},
 url = {https://proceedings.neurips.cc/paper_files/paper/1987/hash/f7177163c833dff4b38fc8d2872f1ec6-Abstract.html},
 volume = {0},
 year = {1987}
}


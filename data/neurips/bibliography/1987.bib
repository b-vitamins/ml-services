 publisher = {American Institute of Physics},
 title = {LEARNING BY STATE RECURRENCE DETECTION},
 url = {https://proceedings.neurips.cc/paper_files/paper/1987/hash/182be0c5cdcd5072bb1864cdee4d3d6e-Abstract.html},
 volume = {0},
 year = {1987}

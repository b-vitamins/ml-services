 openalex = {W2126763700},
 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2012/file/43ec517d68b6edd3015b3edc9a11367b-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {Putting Bayes to sleep},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/43ec517d68b6edd3015b3edc9a11367b-Abstract.html},
 volume = {25},

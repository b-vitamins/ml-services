 openalex = {W2133933452},
 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2012/file/fc8001f834f6a5f0561080d134d53d29-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {On Lifting the Gibbs Sampling Algorithm},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/fc8001f834f6a5f0561080d134d53d29-Abstract.html},
 volume = {25},

 metadata = {https://proceedings.neurips.cc/paper_files/paper/2012/file/5751ec3e9a4feab575962e78e006250d-Metadata.json},
 openalex = {W2267911020},
 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2012/file/5751ec3e9a4feab575962e78e006250d-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {Stochastic optimization and sparse statistical recovery: Optimal algorithms for high dimensions},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/5751ec3e9a4feab575962e78e006250d-Abstract.html},
 volume = {25},

 publisher = {Curran Associates, Inc.},
 supplemental = {https://proceedings.neurips.cc/paper_files/paper/2012/file/85422afb467e9456013a2a51d4dff702-Supplemental.zip},
 title = {Delay Compensation with Dynamical Synapses},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/85422afb467e9456013a2a51d4dff702-Abstract.html},
 volume = {25},

 openalex = {W2136701549},
 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2012/file/8df707a948fac1b4a0f97aa554886ec8-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {Forward-Backward Activation Algorithm for Hierarchical Hidden Markov Models},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/8df707a948fac1b4a0f97aa554886ec8-Abstract.html},
 volume = {25},
 year = {2012}

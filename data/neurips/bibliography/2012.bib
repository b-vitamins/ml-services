 publisher = {Curran Associates, Inc.},
 title = {Learning Label Trees for Probabilistic Modelling of Implicit Feedback},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/0829424ffa0d3a2547b6c9622c77de03-Abstract.html},
 volume = {25},
 year = {2012}

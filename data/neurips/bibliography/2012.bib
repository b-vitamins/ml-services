 editor = {F. Pereira and C.J. Burges and L. Bottou and K.Q. Weinberger},
 metadata = {https://proceedings.neurips.cc/paper_files/paper/2012/file/26e359e83860db1d11b6acca57d8ea88-Metadata.json},
 openalex = {W2133434696},
 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2012/file/26e359e83860db1d11b6acca57d8ea88-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {Shifting Weights: Adapting Object Detectors from Image to Video},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/26e359e83860db1d11b6acca57d8ea88-Abstract.html},

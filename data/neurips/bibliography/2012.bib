 openalex = {W2100178754},
 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2012/file/4476b929e30dd0c4e8bdbcc82c6ba23a-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {Sparse Approximate Manifolds for Differential Geometric MCMC},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/4476b929e30dd0c4e8bdbcc82c6ba23a-Abstract.html},
 volume = {25},

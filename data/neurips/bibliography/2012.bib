 publisher = {Curran Associates, Inc.},
 title = {Adaptive Learning of Smoothing Functions: Application to Electricity Load Forecasting},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/b571ecea16a9824023ee1af16897a582-Abstract.html},
 volume = {25},
 year = {2012}

 pdf = {https://proceedings.neurips.cc/paper_files/paper/2012/file/3435c378bb76d4357324dd7e69f3cd18-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {Statistical Consistency of Ranking Methods in A Rank-Differentiable Probability Space},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/3435c378bb76d4357324dd7e69f3cd18-Abstract.html},

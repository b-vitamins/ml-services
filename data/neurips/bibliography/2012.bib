 publisher = {Curran Associates, Inc.},
 title = {Multilabel Classification using Bayesian Compressed Sensing},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/e1d5be1c7f2f456670de3d53c7b54f4a-Abstract.html},
 volume = {25},
 year = {2012}

 publisher = {Curran Associates, Inc.},
 title = {Cocktail Party Processing via Structured Prediction},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/76dc611d6ebaafc66cc0879c71b5db5c-Abstract.html},
 volume = {25},

 publisher = {Curran Associates, Inc.},
 title = {Memorability of Image Regions},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/e9dae45ec08b498f7e1af247757c9b35-Abstract.html},
 volume = {25},
 year = {2012}

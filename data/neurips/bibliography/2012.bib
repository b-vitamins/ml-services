 supplemental = {https://proceedings.neurips.cc/paper_files/paper/2012/file/ad13a2a07ca4b7642959dc0c4c740ab6-Supplemental.zip},
 title = {Identification of Recurrent Patterns in the Activation of Brain Networks},

 pdf = {https://proceedings.neurips.cc/paper_files/paper/2012/file/0deb1c54814305ca9ad266f53bc82511-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {Timely Object Recognition},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/0deb1c54814305ca9ad266f53bc82511-Abstract.html},
 volume = {25},

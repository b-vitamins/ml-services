 publisher = {Curran Associates, Inc.},
 title = {Diffusion Decision Making for Adaptive k-Nearest Neighbor Classification},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/6395ebd0f4b478145ecfbaf939454fa4-Abstract.html},
 volume = {25},

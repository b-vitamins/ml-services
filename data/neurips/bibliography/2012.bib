 pdf = {https://proceedings.neurips.cc/paper_files/paper/2012/file/f197002b9a0853eca5e046d9ca4663d5-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {Globally Convergent Dual MAP LP Relaxation Solvers using Fenchel-Young Margins},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/f197002b9a0853eca5e046d9ca4663d5-Abstract.html},
 volume = {25},

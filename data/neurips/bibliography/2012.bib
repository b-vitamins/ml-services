 publisher = {Curran Associates, Inc.},
 title = {Random Utility Theory for Social Choice},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/a512294422de868f8474d22344636f16-Abstract.html},
 volume = {25},
 year = {2012}

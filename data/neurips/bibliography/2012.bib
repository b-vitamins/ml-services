 openalex = {W2144438170},
 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2012/file/4588e674d3f0faf985047d4c3f13ed0d-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {Bayesian active learning with localized priors for fast receptive field characterization},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/4588e674d3f0faf985047d4c3f13ed0d-Abstract.html},
 volume = {25},

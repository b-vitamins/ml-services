 openalex = {W2111938016},
 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2012/file/7e7e69ea3384874304911625ac34321c-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {Semi-supervised Eigenvectors for Locally-biased Learning},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/7e7e69ea3384874304911625ac34321c-Abstract.html},

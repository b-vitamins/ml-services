 publisher = {Curran Associates, Inc.},
 title = {Learning with Recursive Perceptual Representations},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/70222949cc0db89ab32c9969754d4758-Abstract.html},
 volume = {25},

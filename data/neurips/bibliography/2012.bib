 openalex = {W2167247347},
 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2012/file/50f3f8c42b998a48057e9d33f4144b8b-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {On the (Non-)existence of Convex, Calibrated Surrogate Losses for Ranking},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/50f3f8c42b998a48057e9d33f4144b8b-Abstract.html},
 volume = {25},

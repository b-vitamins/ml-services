 publisher = {Curran Associates, Inc.},
 title = {Learning curves for multi-task Gaussian process regression},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/dd8eb9f23fbd362da0e3f4e70b878c16-Abstract.html},
 volume = {25},
 year = {2012}

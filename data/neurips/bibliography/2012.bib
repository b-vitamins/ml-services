 openalex = {W2145721327},
 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2012/file/a8240cb8235e9c493a0c30607586166c-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {Dip-means: an incremental clustering method for estimating the number of clusters},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/a8240cb8235e9c493a0c30607586166c-Abstract.html},

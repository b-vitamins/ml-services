 pdf = {https://proceedings.neurips.cc/paper_files/paper/2012/file/ef50c335cca9f340bde656363ebd02fd-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {Pointwise Tracking the Optimal Regression Function},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/ef50c335cca9f340bde656363ebd02fd-Abstract.html},

 publisher = {Curran Associates, Inc.},
 title = {Emergence of Object-Selective Features in Unsupervised Feature Learning},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/39e4973ba3321b80f37d9b55f63ed8b8-Abstract.html},
 volume = {25},

 publisher = {Curran Associates, Inc.},
 title = {Analog readout for optical reservoir computers},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/250cf8b51c773f3f8dc8b4be867a9a02-Abstract.html},
 volume = {25},

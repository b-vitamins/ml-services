 metadata = {https://proceedings.neurips.cc/paper_files/paper/2012/file/c26820b8a4c1b3c2aa868d6d57e14a79-Metadata.json},
 openalex = {W2153412625},
 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2012/file/c26820b8a4c1b3c2aa868d6d57e14a79-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {Priors for Diversity in Generative Latent Variable Models},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/c26820b8a4c1b3c2aa868d6d57e14a79-Abstract.html},
 volume = {25},

 publisher = {Curran Associates, Inc.},
 title = {Multiple Choice Learning: Learning to Produce Multiple Structured Outputs},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/cfbce4c1d7c425baf21d6b6f2babe6be-Abstract.html},
 volume = {25},

 publisher = {Curran Associates, Inc.},
 supplemental = {https://proceedings.neurips.cc/paper_files/paper/2012/file/310dcbbf4cce62f762a2aaa148d556bd-Supplemental.zip},
 title = {Learning about Canonical Views from Internet Image Collections},

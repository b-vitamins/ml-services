 title = {Exploration in Model-based Reinforcement Learning by Empirically Estimating Learning Progress},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/a0a080f42e6f13b3a2df133f073095dd-Abstract.html},

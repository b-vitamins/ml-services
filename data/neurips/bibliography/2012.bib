 pdf = {https://proceedings.neurips.cc/paper_files/paper/2012/file/98dce83da57b0395e163467c9dae521b-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {A new metric on the manifold of kernel matrices with application to matrix geometric means},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/98dce83da57b0395e163467c9dae521b-Abstract.html},
 volume = {25},
 year = {2012}

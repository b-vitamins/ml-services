 pdf = {https://proceedings.neurips.cc/paper_files/paper/2012/file/92c8c96e4c37100777c7190b76d28233-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {Ensemble weighted kernel estimators for multivariate entropy estimation},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/92c8c96e4c37100777c7190b76d28233-Abstract.html},
 volume = {25},

 publisher = {Curran Associates, Inc.},
 supplemental = {https://proceedings.neurips.cc/paper_files/paper/2012/file/801c14f07f9724229175b8ef8b4585a8-Supplemental.zip},
 title = {Approximating Equilibria in Sequential Auctions with Incomplete Information and Multi-Unit Demand},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/801c14f07f9724229175b8ef8b4585a8-Abstract.html},

 openalex = {W2293140760},
 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2012/file/8d6dc35e506fc23349dd10ee68dabb64-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 supplemental = {https://proceedings.neurips.cc/paper_files/paper/2012/file/8d6dc35e506fc23349dd10ee68dabb64-Supplemental.zip},
 title = {Repulsive Mixtures},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/8d6dc35e506fc23349dd10ee68dabb64-Abstract.html},
 volume = {25},
 year = {2012}

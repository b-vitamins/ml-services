 openalex = {W2106636782},
 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2012/file/33e8075e9970de0cfea955afd4644bb2-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {Recognizing Activities by Attribute Dynamics},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/33e8075e9970de0cfea955afd4644bb2-Abstract.html},
 volume = {25},

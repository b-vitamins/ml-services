 openalex = {W2168192936},
 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2012/file/fb60d411a5c5b72b2e7d3527cfc84fd0-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {Perceptron Learning of SAT},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/fb60d411a5c5b72b2e7d3527cfc84fd0-Abstract.html},

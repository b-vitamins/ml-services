 openalex = {W2168156815},
 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2012/file/5e76bef6e019b2541ff53db39f407a98-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {Entropy Estimations Using Correlated Symmetric Stable Random Projections},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/5e76bef6e019b2541ff53db39f407a98-Abstract.html},

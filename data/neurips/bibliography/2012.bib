 publisher = {Curran Associates, Inc.},
 supplemental = {https://proceedings.neurips.cc/paper_files/paper/2012/file/c0c7c76d30bd3dcaefc96f40275bdc0a-Supplemental.zip},
 title = {Dynamic Pruning of Factor Graphs for Maximum Marginal Prediction},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/c0c7c76d30bd3dcaefc96f40275bdc0a-Abstract.html},

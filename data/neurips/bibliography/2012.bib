 publisher = {Curran Associates, Inc.},
 title = {Transelliptical Component Analysis},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/de3f712d1a02c5fb481a7a99b0da7fa3-Abstract.html},
 volume = {25},

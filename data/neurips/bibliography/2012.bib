 publisher = {Curran Associates, Inc.},
 title = {Calibrated Elastic Regularization in Matrix Completion},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/e46de7e1bcaaced9a54f1e9d0d2f800d-Abstract.html},
 volume = {25},

 metadata = {https://proceedings.neurips.cc/paper_files/paper/2012/file/c9f95a0a5af052bffce5c89917335f67-Metadata.json},
 openalex = {W2126394519},
 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2012/file/c9f95a0a5af052bffce5c89917335f67-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {Learning with Target Prior},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/c9f95a0a5af052bffce5c89917335f67-Abstract.html},
 volume = {25},

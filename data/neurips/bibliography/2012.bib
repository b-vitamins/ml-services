 title = {Efficient Monte Carlo Counterfactual Regret Minimization in Games with Many Player Actions},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/3df1d4b96d8976ff5986393e8767f5b2-Abstract.html},

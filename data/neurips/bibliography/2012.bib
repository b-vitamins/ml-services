 publisher = {Curran Associates, Inc.},
 title = {Factoring nonnegative matrices with linear programs},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/08c5433a60135c32e34f46a71175850c-Abstract.html},
 volume = {25},

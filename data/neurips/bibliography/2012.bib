 openalex = {W2142880609},
 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2012/file/d759175de8ea5b1d9a2660e45554894f-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {Learning Networks of Heterogeneous Influence},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/d759175de8ea5b1d9a2660e45554894f-Abstract.html},
 volume = {25},
 year = {2012}

 metadata = {https://proceedings.neurips.cc/paper_files/paper/2012/file/6c8dba7d0df1c4a79dd07646be9a26c8-Metadata.json},
 openalex = {W2158334272},
 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2012/file/6c8dba7d0df1c4a79dd07646be9a26c8-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 supplemental = {https://proceedings.neurips.cc/paper_files/paper/2012/file/6c8dba7d0df1c4a79dd07646be9a26c8-Supplemental.zip},
 title = {Near-Optimal MAP Inference for Determinantal Point Processes},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/6c8dba7d0df1c4a79dd07646be9a26c8-Abstract.html},
 volume = {25},

 pdf = {https://proceedings.neurips.cc/paper_files/paper/2012/file/d6baf65e0b240ce177cf70da146c8dc8-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 title = {Assessing Blinding in Clinical Trials},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/d6baf65e0b240ce177cf70da146c8dc8-Abstract.html},
 volume = {25},

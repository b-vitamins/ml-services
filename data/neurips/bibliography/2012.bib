 publisher = {Curran Associates, Inc.},
 title = {Imitation Learning by Coaching},
 url = {https://proceedings.neurips.cc/paper_files/paper/2012/hash/2dffbc474aa176b6dc957938c15d0c8b-Abstract.html},
 volume = {25},

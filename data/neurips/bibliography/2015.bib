 review = {https://proceedings.neurips.cc/paper_files/paper/2015/file/f0bbac6fa079f1e00b2c14c1d3c6ccf0-Reviews.html},
 title = {On elicitation complexity},
 url = {https://proceedings.neurips.cc/paper_files/paper/2015/hash/f0bbac6fa079f1e00b2c14c1d3c6ccf0-Abstract.html},
 volume = {28},

 publisher = {Curran Associates, Inc.},
 review = {https://proceedings.neurips.cc/paper_files/paper/2015/file/bad5f33780c42f2588878a9d07405083-Reviews.html},
 title = {Generalization in Adaptive Data Analysis and Holdout Reuse},
 url = {https://proceedings.neurips.cc/paper_files/paper/2015/hash/bad5f33780c42f2588878a9d07405083-Abstract.html},
 volume = {28},

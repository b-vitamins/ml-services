 review = {https://proceedings.neurips.cc/paper_files/paper/2015/file/645098b086d2f9e1e0e939c27f9f2d6f-Reviews.html},
 title = {Fast lifted MAP inference via partitioning},
 url = {https://proceedings.neurips.cc/paper_files/paper/2015/hash/645098b086d2f9e1e0e939c27f9f2d6f-Abstract.html},

 publisher = {Curran Associates, Inc.},
 review = {https://proceedings.neurips.cc/paper_files/paper/2015/file/fcdf25d6e191893e705819b177cddea0-Reviews.html},
 title = {Local causal discovery of direct causes and effects},
 url = {https://proceedings.neurips.cc/paper_files/paper/2015/hash/fcdf25d6e191893e705819b177cddea0-Abstract.html},

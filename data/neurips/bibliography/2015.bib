 title = {Covariance-Controlled Adaptive Langevin Thermostat for Large-Scale Bayesian Sampling},
 url = {https://proceedings.neurips.cc/paper_files/paper/2015/hash/6f4922f45568161a8cdf4ad2299f6d23-Abstract.html},

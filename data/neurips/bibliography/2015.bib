 publisher = {Curran Associates, Inc.},
 review = {https://proceedings.neurips.cc/paper_files/paper/2015/file/861dc9bd7f4e7dd3cccd534d0ae2a2e9-Reviews.html},
 supplemental = {https://proceedings.neurips.cc/paper_files/paper/2015/file/861dc9bd7f4e7dd3cccd534d0ae2a2e9-Supplemental.zip},
 title = {A normative theory of adaptive dimensionality reduction in neural networks},

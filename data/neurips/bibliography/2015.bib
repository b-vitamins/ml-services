 openalex = {W1877007425},
 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2015/file/89f03f7d02720160f1b04cf5b27f5ccb-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 review = {https://proceedings.neurips.cc/paper_files/paper/2015/file/89f03f7d02720160f1b04cf5b27f5ccb-Reviews.html},
 supplemental = {https://proceedings.neurips.cc/paper_files/paper/2015/file/89f03f7d02720160f1b04cf5b27f5ccb-Supplemental.zip},
 title = {A Pseudo-Euclidean Iteration for Optimal Recovery in Noisy ICA},
 url = {https://proceedings.neurips.cc/paper_files/paper/2015/hash/89f03f7d02720160f1b04cf5b27f5ccb-Abstract.html},

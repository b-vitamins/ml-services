 review = {https://proceedings.neurips.cc/paper_files/paper/2015/file/f3f1b7fc5a8779a9e618e1f23a7b7860-Reviews.html},
 title = {Beyond Sub-Gaussian Measurements: High-Dimensional Structured Estimation with Sub-Exponential Designs.},

 publisher = {Curran Associates, Inc.},
 review = {https://proceedings.neurips.cc/paper_files/paper/2015/file/aa486f25175cbdc3854151288a645c19-Reviews.html},
 title = {Estimating Jaccard index with missing observations: a matrix calibration approach},
 url = {https://proceedings.neurips.cc/paper_files/paper/2015/hash/aa486f25175cbdc3854151288a645c19-Abstract.html},

 supplemental = {https://proceedings.neurips.cc/paper_files/paper/2016/file/aace49c7d80767cffec0e513ae886df0-Supplemental.zip},
 title = {"Short-Dot": Computing Large Linear Transforms Distributedly Using Coded Short Dot Products},
 url = {https://proceedings.neurips.cc/paper_files/paper/2016/hash/aace49c7d80767cffec0e513ae886df0-Abstract.html},
 volume = {29},

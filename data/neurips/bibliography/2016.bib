 openalex = {W2542487480},
 pages = {},
 pdf = {https://proceedings.neurips.cc/paper_files/paper/2016/file/c88d8d0a6097754525e02c2246d8d27f-Paper.pdf},
 publisher = {Curran Associates, Inc.},
 review = {https://proceedings.neurips.cc/paper_files/paper/2016/file/c88d8d0a6097754525e02c2246d8d27f-Reviews.html},
 title = {New liftable classes for first-order probabilistic inference},
 url = {https://proceedings.neurips.cc/paper_files/paper/2016/hash/c88d8d0a6097754525e02c2246d8d27f-Abstract.html},

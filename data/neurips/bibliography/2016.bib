 supplemental = {https://proceedings.neurips.cc/paper_files/paper/2016/file/fe70c36866add1572a8e2b96bfede7bf-Supplemental.zip},
 title = {Balancing Suspense and Surprise: Timely Decision Making with Endogenous Information Acquisition},

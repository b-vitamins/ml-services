 publisher = {Curran Associates, Inc.},
 review = {https://proceedings.neurips.cc/paper_files/paper/2016/file/5b8add2a5d98b1a652ea7fd72d942dac-Reviews.html},
 supplemental = {https://proceedings.neurips.cc/paper_files/paper/2016/file/5b8add2a5d98b1a652ea7fd72d942dac-Supplemental.zip},
 title = {Solving Random Systems of Quadratic Equations via Truncated Generalized Gradient Flow},
 url = {https://proceedings.neurips.cc/paper_files/paper/2016/hash/5b8add2a5d98b1a652ea7fd72d942dac-Abstract.html},

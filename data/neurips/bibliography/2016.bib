 supplemental = {https://proceedings.neurips.cc/paper_files/paper/2016/file/3def184ad8f4755ff269862ea77393dd-Supplemental.zip},
 title = {Minimizing Regret on Reflexive Banach Spaces and Nash Equilibria in Continuous Zero-Sum Games},

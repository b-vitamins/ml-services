 publisher = {Curran Associates, Inc.},
 review = {https://proceedings.neurips.cc/paper_files/paper/2016/file/03e7ef47cee6fa4ae7567394b99912b7-Reviews.html},
 supplemental = {https://proceedings.neurips.cc/paper_files/paper/2016/file/03e7ef47cee6fa4ae7567394b99912b7-Supplemental.zip},
 title = {Achieving Budget-optimality with Adaptive Schemes in Crowdsourcing},
 url = {https://proceedings.neurips.cc/paper_files/paper/2016/hash/03e7ef47cee6fa4ae7567394b99912b7-Abstract.html},

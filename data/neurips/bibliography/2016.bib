 supplemental = {https://proceedings.neurips.cc/paper_files/paper/2016/file/8c01a75941549a705cf7275e41b21f0d-Supplemental.zip},
 title = {Generalization of ERM in Stochastic Convex Optimization: The Dimension Strikes Back},
 url = {https://proceedings.neurips.cc/paper_files/paper/2016/hash/8c01a75941549a705cf7275e41b21f0d-Abstract.html},

 supplemental = {https://proceedings.neurips.cc/paper_files/paper/2016/file/b51a15f382ac914391a58850ab343b00-Supplemental.zip},
 title = {Phased Exploration with Greedy Exploitation in Stochastic Combinatorial Partial Monitoring Games},

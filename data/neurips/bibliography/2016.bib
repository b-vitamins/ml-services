 publisher = {Curran Associates, Inc.},
 review = {https://proceedings.neurips.cc/paper_files/paper/2016/file/7fec306d1e665bc9c748b5d2b99a6e97-Reviews.html},
 supplemental = {https://proceedings.neurips.cc/paper_files/paper/2016/file/7fec306d1e665bc9c748b5d2b99a6e97-Supplemental.zip},
 title = {Adaptive optimal training of animal behavior},

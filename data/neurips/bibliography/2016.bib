 publisher = {Curran Associates, Inc.},
 review = {https://proceedings.neurips.cc/paper_files/paper/2016/file/f48c04ffab49ff0e5d1176244fdfb65c-Reviews.html},
 supplemental = {https://proceedings.neurips.cc/paper_files/paper/2016/file/f48c04ffab49ff0e5d1176244fdfb65c-Supplemental.zip},
 title = {Agnostic estimation for misspecified phase retrieval models},
